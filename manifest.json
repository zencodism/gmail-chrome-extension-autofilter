{
    "name": "Gmail autofilter",
    "version": "0.1",
    "description": "Gmail chrome extension with automatic filters",
    "content_scripts": [
    {
        "matches": ["https://mail.google.com/*"],
        "js": ["content.js"]
    }
    ],
    "web_accessible_resources": [
        "jquery-1.10.2.min.js",
        "gmail.js",
        "filterapi.js",
        "main.js"
    ],
    "manifest_version": 2
}
